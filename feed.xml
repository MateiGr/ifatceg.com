<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
<title>Tactics Journal</title>
<subtitle>Analyzing football tactics by Kyle Boas</subtitle>
<link rel="alternate" type="text/html" href="https://tacticsjournal.com/" />
<link rel="self" type="application/atom+xml" href="https://tacticsjournal.com/feed.xml" />
<id>https://tacticsjournal.com/feed.xml</id>
    {% for post in site.posts %}
    <entry>
        <link rel="alternate" type="text/html" href="{{ site.url }}{{ post.url }}" />
        <title>{{ post.title }}</title>
        <published>{{ post.date | date_to_rfc822 }}</published>
        <author>
          <name>Kyle Boas</name>
          <uri>http://tacticsjournal.com/</uri>
        </author>
        <summary type="text">{{ post.excerpt | xml_escape }}</summary>
        <content type="html" xml:base="https://tacticsjournal.com/" xml:lang="en"><![CDATA[{{ post.content | xml_escape | replace: 'javascript||' | replace: 'shell||' | replace: 'ruby||' | replace: 'html||' | replace: 'src=&quot;/', 'src=&quot;https://tacticsjournal.com/' | replace: 'href=&quot;/', 'href=&quot;https://tacticsjournal.com/' }}]]></content>
    </entry>
    {% endfor %}
</channel>
</rss>
